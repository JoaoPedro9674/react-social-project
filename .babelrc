{
    "presets": [
        "es2015",
        "react",
        "stage-0",
        "es2015-native-modules",
        "es2015-webpack"
    ],
    "plugins": ["transform-class-properties", "transform-runtime"],
    "env": {
        "development": {
            "plugins": [
                "typecheck",
            ],
            "presets": [
                "react-hmre"
            ]
        },
        "testing": {
          "plugins": [
              "typecheck",
          ],
        }
    }
}
